<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Properties"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                   <fileimage jcr:primaryType="nt:unstructured"
												        sling:resourceType="granite/ui/components/foundation/form/fileupload"
												        autoStart="{Boolean}false" 
												        allowUpload="{Boolean}false" 
												        class="cq-droptarget"
												        fieldLabel="image" 
												        fileNameParameter="./image/fileName"
												        fileReferenceParameter="./image/fileReference" 
												        mimeTypes="[image]"
												        multiple="{Boolean}false"
												        name="./image/file" 
												        uploadUrl="${suffix.path}"
												        useHTML5="{Boolean}true" />
															       <resType jcr:primaryType="nt:unstructured"
																	        sling:resourceType="granite/ui/components/foundation/form/hidden"
																	        name="./image/sling:resourceType" 
																	        value="foundation/components/image" />
																	        
										 <fileimage1 jcr:primaryType="nt:unstructured"
												        sling:resourceType="granite/ui/components/foundation/form/fileupload"
												        autoStart="{Boolean}false" 
												        allowUpload="{Boolean}false" 
												        class="cq-droptarget"
												        fieldLabel="image one" 
												        fileNameParameter="./image1/fileName"
												        fileReferenceParameter="./image1/fileReference" 
												        mimeTypes="[image]"
												        multiple="{Boolean}false"
												        name="./image1/file" 
												        uploadUrl="${suffix.path}"
												        useHTML5="{Boolean}true" />
															       <resType jcr:primaryType="nt:unstructured"
																	        sling:resourceType="granite/ui/components/foundation/form/hidden"
																	        name="./image1/sling:resourceType" 
																	        value="foundation/components/image1" />
										
										<fileimage2 jcr:primaryType="nt:unstructured"
												        sling:resourceType="granite/ui/components/foundation/form/fileupload"
												        autoStart="{Boolean}false" 
												        allowUpload="{Boolean}false" 
												        class="cq-droptarget"
												        fieldLabel="image second" 
												        fileNameParameter="./image2/fileName"
												        fileReferenceParameter="./image2/fileReference" 
												        mimeTypes="[image]"
												        multiple="{Boolean}false"
												        name="./image2/file" 
												        uploadUrl="${suffix.path}"
												        useHTML5="{Boolean}true" />
															       <resType jcr:primaryType="nt:unstructured"
																	        sling:resourceType="granite/ui/components/foundation/form/hidden"
																	        name="./image1/sling:resourceType" 
																	        value="foundation/components/image2" />
																	        
										<fileimage3 jcr:primaryType="nt:unstructured"
												        sling:resourceType="granite/ui/components/foundation/form/fileupload"
												        autoStart="{Boolean}false" 
												        allowUpload="{Boolean}false" 
												        class="cq-droptarget"
												        fieldLabel="image three" 
												        fileNameParameter="./image3/fileName"
												        fileReferenceParameter="./image3/fileReference" 
												        mimeTypes="[image]"
												        multiple="{Boolean}false"
												        name="./image3/file" 
												        uploadUrl="${suffix.path}"
												        useHTML5="{Boolean}true" />
															       <resType jcr:primaryType="nt:unstructured"
																	        sling:resourceType="granite/ui/components/foundation/form/hidden"
																	        name="./image1/sling:resourceType" 
																	        value="foundation/components/image3" />
                </items>
            </column>
        </items>
    </content>
</jcr:root>
